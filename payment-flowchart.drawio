        </mxCell>
        <mxCell id="PcXyaJ6zOk0AMBvOgHcU-53" value="Step 5" style="text;html=1;align=center;verticalAlign=middle;whiteSpace=wrap;rounded=0;" vertex="1" parent="1">
          <mxGeometry x="550" y="235" width="60" height="30" as="geometry" />
